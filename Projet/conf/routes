# Routes
# Ce fichier definit toute  les  route
# ~~~~

# Route référant au différante page:
GET     /                           controllers.HomeController.index
GET     /index                      controllers.HomeController.index
GET     /profile                    controllers.HomeController.login(formulaire : Request )
POST     /profile                   controllers.HomeController.profile(formulaire : Request )
GET     /login                    	controllers.HomeController.login(formulaire : Request)
POST     /login                    	controllers.HomeController.login(formulaire : Request)
GET     /contact                    controllers.HomeController.contact


# Route Pour  les  médiat Public
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET     /images/*file               controllers.Assets.at(path="/public/images", file)
GET     /javascripts/*file          controllers.Assets.at(path="/public/javascripts", file)

#Faire des  post çaserait plus  safe
